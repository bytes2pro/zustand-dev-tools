{
  "name": "@rte/vue-ui",
  "version": "0.0.0",
  "private": true,
  "description": "Vue 3 UI components",
  "license": "MIT",
  "main": "dist/vue-ui.cjs.js",
  "module": "dist/vue-ui.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-ui.esm.js",
      "require": "./dist/vue-ui.cjs.js"
    }
  },
  "scripts": {
    "build": "vite build && vue-tsc -p tsconfig.build.json --declaration --emitDeclarationOnly",
    "typecheck": "vue-tsc -p tsconfig.json --noEmit",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "rimraf": "^5.0.5",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.19"
  }
}