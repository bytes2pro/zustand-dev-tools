{
  "name": "@rte/svelte-ui",
  "version": "0.0.0",
  "private": false,
  "description": "Svelte UI components",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "svelte": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "build": "vite build && tsc -p tsconfig.build.json --emitDeclarationOnly",
    "typecheck": "svelte-check --tsconfig ./tsconfig.json --fail-on-warnings",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "svelte": ">=4.0.0 || ^5.0.0-next"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "rimraf": "^5.0.5",
    "svelte": "^4.2.0",
    "svelte-preprocess": "^5.1.4",
    "svelte-check": "^4.0.0",
    "typescript": "^5.4.0",
    "vite": "^5.2.0"
  }
}