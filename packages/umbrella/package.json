{
	"name": "@rte/your-package",
	"version": "0.0.0",
	"private": false,
	"description": "Umbrella package that re-exports framework builds",
	"license": "MIT",
	"files": [
		"dist"
	],
	"sideEffects": false,
	"exports": {
		"./react": {
			"types": "./dist/react/index.d.ts",
			"import": "./dist/react/index.esm.js",
			"require": "./dist/react/index.cjs.js"
		},
		"./next": {
			"types": "./dist/next/index.d.ts",
			"import": "./dist/next/index.mjs",
			"require": "./dist/next/index.js"
		},
		"./nuxt": {
			"types": "./dist/nuxt/index.d.ts",
			"import": "./dist/nuxt/index.esm.js",
			"require": "./dist/nuxt/index.cjs.js"
		},
		"./vue": {
			"types": "./dist/vue/index.d.ts",
			"import": "./dist/vue/vue-ui.es.js",
			"require": "./dist/vue/vue-ui.cjs.js"
		},
		"./solid": {
			"types": "./dist/solid/index.d.ts",
			"import": "./dist/solid/solid-ui.es.js",
			"require": "./dist/solid/solid-ui.cjs.js"
		},
		"./preact": {
			"types": "./dist/preact/index.d.ts",
			"import": "./dist/preact/index.esm.js",
			"require": "./dist/preact/index.cjs.js"
		},
		"./lit": {
			"types": "./dist/lit/index.d.ts",
			"import": "./dist/lit/index.esm.js",
			"require": "./dist/lit/index.cjs.js"
		},
		"./svelte": {
			"types": "./dist/svelte/index.d.ts",
			"import": "./dist/svelte/index.esm.js",
			"require": "./dist/svelte/index.cjs.js"
		}
	},
	"scripts": {
		"build": "node ../../scripts/build-umbrella.mjs",
		"clean": "rimraf dist"
	},
	"devDependencies": {
		"rimraf": "^5.0.5"
	}
}