{
	"name": "@rte/nuxt-ui",
	"version": "0.0.0",
	"private": true,
	"description": "Nuxt-ready Vue components",
	"license": "MIT",
	"main": "dist/index.cjs.js",
	"module": "dist/index.esm.js",
	"types": "dist/index.d.ts",
	"sideEffects": false,
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.esm.js",
			"require": "./dist/index.cjs.js"
		}
	},
	"scripts": {
		"build": "vite build && vue-tsc -p tsconfig.build.json --declaration --emitDeclarationOnly",
		"typecheck": "vue-tsc -p tsconfig.json --noEmit",
		"clean": "rimraf dist"
	},
	"peerDependencies": {
		"vue": ">=3.3.0",
		"nuxt": ">=3.0.0"
	},
	"peerDependenciesMeta": {
		"nuxt": {
			"optional": true
		}
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^5.0.4",
		"rimraf": "^5.0.5",
		"vite": "^5.2.0",
		"vue-tsc": "^2.0.19"
	}
}